{
  "name": "portfolio-website",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
  "build:scss": "sass src/css/styles.scss dist/css/styles.css",
  "build:tailwind": "tailwindcss -i dist/css/styles.css -o dist/css/styles.css",
  "build": "npm run build:scss && npm run build:tailwind",
  "watch:scss": "sass src/css/styles.scss dist/css/styles.css --watch",
  "watch:tailwind": "tailwindcss -i dist/css/styles.css -o dist/css/styles.css --watch",
  "start": "npm-run-all --parallel watch:scss watch:tailwind"
},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "sass": "^1.80.6",
    "tailwindcss": "^3.4.14"
  }
}
